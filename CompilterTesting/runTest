#!/bin/bash

mx vm -Xbootclasspath/a:../lib/graal-query.jar -G:-RemoveNeverExecutedCode \
    -G:+UseGraalQueries -G:-InlineDuringParsing -G:-OptDeoptimizationGrouping \
    -G:SmallCompiledLowLevelGraphSize=0 \
    -cp ../lib/junit-4.11.jar:../lib/hamcrest-core-1.3.jar:bin \
    org.junit.runner.JUnitCore $@
